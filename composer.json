{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework starter",
    "keywords": ["framework", "laravel", "laravel starter", "laravel dashboard"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.40",
        "laravel/tinker": "^2.5",
        "laravel/socialite": "^5.0",
        "alexusmai/laravel-file-manager": "^2.5",
        "arcanedev/log-viewer": "^8.0",
        "lavary/laravel-menu": "^1.7",
        "nwidart/laravel-modules": "^8.0",
        "mews/purifier": "^3.3",
        "predis/predis": "^1.1",
        "spatie/laravel-activitylog": "^3.16",
        "spatie/laravel-backup": "^6.11",
        "spatie/laravel-feed": "^3.2",
        "spatie/laravel-html": "^2.28",
        "spatie/laravel-medialibrary": "^8.10",
        "spatie/laravel-permission": "^3.17",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.2",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
